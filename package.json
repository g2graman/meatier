{
  "name": "meatier",
  "version": "0.10.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/g2graman/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/g2graman/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env NODE_ENV=development ts-node ./src/server/server.ts",
    "prod": "npx cross-env NODE_ENV=production ts-node ./src/server/server.ts",
    "build": "npx rimraf build && npx concurrently \"npm run build:client\" \"npm run build:server\"",
    "bs": "npx rimraf build && npx concurrently \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "quickstart": "npx rimraf build && npx concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "build:client": "npx cross-env NODE_ENV=production npx -n '-r ts-node/register' webpack --config ./webpack/webpack.config.prod.ts",
    "build:server": "npx cross-env NODE_ENV=production npx -n '-r ts-node/register' webpack ./webpack/webpack.config.server.ts",
    "build:db": "node ./src/server/database/setupDB.babel.js",
    "update:db": "node ./src/server/database/setupDB.babel.js true",
    "test": "npx cross-env NODE_ENV=testing ava ./src/**/__tests__/**/*.js --verbose",
    "lint": "npx xo --fix 'src/**/*.js' 'tests/*.js' 'webpack/*.js'",
    "profile": "npx cross-env NODE_ENV=production npx -n '-r ts-node/register' webpack --config ./webpack/webpack.config.prod.ts --profile --json > stats.json"
  },
  "author": "Francesco Gramano <fran.gramano@mail.utoronto.ca>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "ava": "0.14.0",
    "concurrently": "2.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-xo-meatier": "0.2.2",
    "eslint-config-xo-react": "0.7.0",
    "eslint-plugin-react": "5.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "happypack": "^4.0.0-beta.5",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.0.2",
    "react-hot-loader": "^1.3.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
